config_version=5

[application]

config/name="Resource Resaver Demo"
config/description="A small addon that automates re-saving all `.tres` files in your project.
This is useful if you've modified a resource class definition (`.gd` file) and need to update all corresponding `.tres` files.
"
config/version="1.0.0"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[display]

window/dpi/allow_hidpi=false

[editor_plugins]

enabled=PackedStringArray("res://addons/gg_resource_resaver/plugin.cfg")
